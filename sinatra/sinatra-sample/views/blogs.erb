<h1>List of All Blogs</h1>
<div class='blogList'>
	<% @blogs.each do |b| %>

	<li><a href="/blog/<%= b.id %>"><span class='titlePosts' id='postTitle'>Title:</span> <%= b.title %></a></li>
	<li><span class='titlePosts'>Body:</span> <%=b.body %></li>
	<li><span class='titlePosts'>User:</span> <%=b.user.username%> </li>
	<br>
	<% end %>
</div>


